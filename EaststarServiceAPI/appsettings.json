{
  "JwtOptions": {
    "Authority": "https://localhost",
    "Issuer": "https://localhost",
    "Audience": "MyAudience",
    "SecretKey": "ThisIsMy32CharacterSuperSecretKey!"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "LetsEncrypt": {
    "DirectoryPath": "/etc/letsencrypt/live/localhost/",
    "CertificateFileName": "fullchain.pem",
    "PrivateKeyFileName": "privkey.pem"
  }
}
